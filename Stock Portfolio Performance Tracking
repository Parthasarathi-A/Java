class HelloWorld {
    static int querysum(int queries[][], int array[], int querySize, int size) {
        for (int i = 0; i < querySize; i++) {
            int L = queries[i][0];
            int R = queries[i][1];
            int sum = 0;
            for (int j = L; j <= R; j++) {
                sum += array[j];
            }
            System.out.println("Sum for query " + (i + 1) + " (from " + L + " to " + R + "): " + sum);
        }
        return 0;
    }
